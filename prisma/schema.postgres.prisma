// This is your Prisma schema file for PostgreSQL
// Use this for production/staging deployments
// To use: copy this file to schema.prisma or update datasource manually

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// All models remain the same - they're compatible with both SQLite and PostgreSQL
// Just copy the rest of your models from schema.prisma


